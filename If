/*圆柱


#include <stdio.h>

#define PI 3.14  

int main() {
    double r, h;  
    double c, s, v;  

    if (scanf("%lf %lf", &r, &h) != 2 || r <= 0 || h <= 0) {  
        printf("此输入不是圆柱体。\n");
    }
    else {
        c = 2 * PI * r;
        s = PI * r * r;
        v = s * h;
        printf("%.2f %.2f %.2f", c, s, v); 
    }

    return 0;
}


*/
