/*坤兔同笼


#include <stdio.h>

int main() {
    int heads, legs, rabbits, chickens; 
    scanf("%d%d", &heads, &legs); 

    
    chickens = (4 * heads - legs) / 2;
    rabbits = heads - chickens;

    if (legs % 2 == 1 || rabbits < 0 || chickens < 0) {
        printf("No solution!");
        return 0;
    }
    printf("chickens = %d; rabbits = %d", chickens, rabbits);
    return 0;
}


*/




